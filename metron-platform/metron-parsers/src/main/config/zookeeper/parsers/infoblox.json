{
  "parserClassName":"org.apache.metron.parsers.infoblox.GrokInfobloxParser",
  "sensorTopic":"infoblox",
  "parserConfig":
  {
    "grokPath":"/patterns/infoblox",
    "patternLabel":"INFOBLOX",
    "timestampField":"timestamp",
    "dateFormat":"yyyy MMM dd HH:mm:ss"
  },
  "fieldTransformations" : [
    {
      "input" : "timestamp",
      "transformation": "TRANSFORM_TO_UTC_EPOCH",
      "config":
      { "timeZone":"America/New_York" }
    }
  ]
}